<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".SpeedTest_Fragment">


  <TextView
      android:id="@+id/txt_speedtest"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:fontFamily="@font/inter_semibold"
      android:text="Speedtest Accuracy"
      android:textAllCaps="false"
      android:textColor="@color/black"
      android:textSize="30sp"
      android:layout_marginTop="@dimen/_31sdp"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent" />

  <TextView
      android:layout_width="0dp"
      android:layout_height="wrap_content"
      android:fontFamily="@font/inter"
      android:text="On the other hand, we denounce with righteous indignation and dislike men."
      android:textAllCaps="false"
      android:textColor="#686868"
      android:textSize="16sp"
      android:gravity="center"
      android:layout_marginTop="@dimen/_9sdp"
      android:layout_marginHorizontal="@dimen/_15sdp"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/txt_speedtest" />

       


</androidx.constraintlayout.widget.ConstraintLayout>